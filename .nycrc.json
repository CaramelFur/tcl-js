{
  "sourceMap": true,
  "instument": true,
  "reporter": ["text-summary", "lcov", "json"],
  "temp-directory": "./coverage/.nyc_output",
  "report-dir": "./coverage/",
  "extension": [".js", ".jsx", ".ts", ".tsx"],
  "extends": "@istanbuljs/nyc-config-typescript",
  "exclude": ["test/", "**/generated"],

  "check-coverage": true,

  "branches": 95,
  "lines": 95,
  "functions": 95,
  "statements": 95
}
